# solution 1:
class Solution:
    # s 源字符串
    def replaceSpace(self, s):
        # write code here
        result = ''
        for i in s :
            if i == ' ' :
                result += '%20'
            else :
                result += i
        return result


# solution 2:
class Solution:
    # s 源字符串
    def replaceSpace(self, s):
        # write code here
        return s.replace(' ','%20')
